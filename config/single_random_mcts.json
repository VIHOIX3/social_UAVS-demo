{
    "log_file": "output.log",
    "out_dir": "./out",
    "sub_dirs": ["plots", "traj"],
    "gpu": true,
    "gpu_id": 0,

    "seed": 1, 
    
    "game": {
        "num_episodes": 5, 
        "max_steps": 50,
        "num_agents": 1,
        "deterministic": true
    },

    "search_policy": {
        "type": "mcts",
        "setting": "singleagent",
        "c_uct": 1,
        "h_uct": 5,
        "search": "ts",
        "max_time": 10,
        "num_ts": 5
    },

    "social_policy": {
        "type": "random",
        "num_predictions": 10
    },

    "data": {
        "dataset_path": "./data/dataset",
        "dataset_name": "111_days",
        "trajlib_path": "./data/traj_lib_0SI.txt",
        "indexlib_path": "./data/traj_index_0SI.txt",
        "reflib_path": "./data/dic2d.pkl",
        "costmap_path": "./data/dataset/111days1/train"
    },

    "visualization": {
        "visualize": true
    }
}